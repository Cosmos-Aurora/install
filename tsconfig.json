    "compilerOptions": {
      "target": "es2015",
      "lib": ["es2015"],
      "module": "commonjs",
      "moduleResolution": "node",
      "esModuleInterop": true,
      "resolveJsonModule": true,
      "forceConsistentCasingInFileNames": true,
      "isolatedModules": true,
      "strict": true
    }
